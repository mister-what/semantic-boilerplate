{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 4 Chrome versions"]
        },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    [
      "direct-import",
      [
        "react-router",
        "react-router-dom"
      ]
    ],
    "lodash",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
